">\n        <div v-for=\"item in formData.pricingPlanList\" :key=\"item.type\" class=\"mb-10\">\n          <span style=\"width: 150px; display: inline-block\">{{ item.label }} Pricing Plan:</span>\n          <el-select v-model=\"item.schemeId\">\n            <el-option\n              v-for=\"plan in matchPricingList\"\n              :key=\"plan.value\"\n              :label=\"plan.label\"\n              :value=\"plan.value\"\n            ></el-option>\n          </el-select>\n        </div>\n      </el-form-item>\n    </el-form>\n\n    <el-button type=\"primary\" @click=\"save\">Save</el-button>\n    <el-button type=\"primary\" @click=\"cancel\">Cancel</el-button>\n  </div>\n</template>\n\n<script setup>\nimport useCityList from \"@/store/useCityList\";\nimport useFeatureList from \"@/store/useFeatureList\";\nimport dayjs from \"dayjs\";\nimport http from \"@/utils/http\";\n\nconst props = defineProps({\n  formData: {\n    type: Object,\n    default: () => ({}),\n  },\n});\nconst emit = defineEmits([\"save\", \"cancel\"]);\n\nconst router = useRouter();\nconst cityList = useCityList();\nconst featureList = useFeatureList();\nlet pricingList = ref([]);\nconst matchPricingList = computed(() =>\n  pricingList.value.filter(\n    (o) =>\n      (o.startCityId === props.formData.startCityId && o.endCityId === props.formData.endCityId) ||\n      (o.endCityId === props.formData.startCityId && o.startCityId === props.formData.endCityId)\n  )\n);\nlet stationList = ref([]);\nconst pickupStationList = computed(() => stationList.value.filter((o) => o.cityId === props.formData.startCityId));\nconst dropoffStationList = computed(() => stationList.value.filter((o) => o.cityId === props.formData.endCityId));\nconst scheduleTime = computed({\n  set(value) {\n    if (value) {\n      const [startTime, endTime] = value;\n      props.formData.startScheduleTime = startTime;\n      props.formData.endScheduleTime = endTime;\n    } else {\n      props.formData.startScheduleTime = \"\";\n      props.formData.endScheduleTime = \"\";\n    }\n  },\n  get() {\n    return [props.formData.startScheduleTime, props.formData.endScheduleTime];\n  },\n});\nconst ruleForm = ref(null);\nconst locationValidator = (action, item) => (rule, value, callback) => {\n  if (!item.locationStationId) {\n    callback(new Error(`${action} location is required!`));\n  } else {\n    callback();\n  }\n};\nconst baseRules = {\n  subBuslineName: [{ required: true, message: \"Sub busline name is required!\", trigger: \"blur\" }],\n  startCityId: [{ required: true, message: \"Departure city is required!\", trigger: \"blur\" }],\n  endCityId: [{ required: true, message: \"Arrival city is required!\", trigger: \"blur\" }],\n  startTime: [{ required: true, message: \"Time departure is required!\", trigger: \"blur\" }],\n  endTime: [{ required: true, message: \"Time destination is required!\", trigger: \"blur\" }],\n  isSchedule: [\n    {\n      validator(rule, value, callback) {\n        if (value === 1 && (!props.formData.startScheduleTime || !props.formData.endScheduleTime)) {\n          callback(new Error(\"Holiday time is required!\"));\n        } else {\n          callback();\n        }\n      },\n    },\n  ],\n};\nconst formRules = computed(() => {\n  const rules = {\n    ...baseRules,\n  };\n  props.formData.pickupLocation.forEach((item, index) => {\n    rules[`pickup-station-${index}`] = [\n      {\n        validator: locationValidator(\"Pickup\", item),\n        trigger: \"blur\",\n      },\n    ];\n  });\n  props.formData.dropOffLocation.forEach((item, index) => {\n    rules[`dropoff-station-${index}`] = [\n      {\n        validator: locationValidator(\"Dropoff\", item),\n        trigger: \"blur\",\n      },\n    ];\n  });\n  return rules;\n});\n\nwatch(\n  () => matchPricingList,\n  (val) => {\n    const defaultPricing = matchPricingList.value[0]?.value ?? null;\n    props.formData.pricingPlanList.forEach((item) => {\n      if (defaultPricing) {\n        if (!item.schemeId) {\n          item.schemeId = defaultPricing;\n        } else if (!matchPricingList.value.find((o) => o.value === item.schemeId)) {\n          item.schemeId = defaultPricing;\n        }\n      } else {\n        item.schemeId = null;\n      }\n    });\n  },\n  {\n    deep: true,\n    immediate: true,\n  }\n);\n\nconst startCityRef = ref(null);\nconst startCityName = computed(() => {\n  return startCityRef.value?.inputValue;\n});\nconst endCityRef = ref(null);\nconst endCityName = computed(() => {\n  return endCityRef.value.inputValue;\n});\n\nconst todayUnix = dayjs().startOf(\"day\").valueOf();\nconst disabledDate = (time) => {\n  return time.getTime() < todayUnix;\n};\n\nconst getPricingList = () => {\n  http.get(\"/operate/BusOperateSchemeServe/queryList\").then((res) => {\n    const pricing = (res.t || []).map((o) => ({\n      value: o.schemeId,\n      label: o.name,\n      type: o.type,\n      startCityId: o.startCityId,\n      endCityId: o.endCityId,\n    }));\n\n    pricingList.value = pricing;\n  });\n};\ngetPricingList();\nconst addPickupLocation = () => {\n  props.formData.pickupLocation.push({\n    runDays: 0,\n    locationTime: props.formData.startTime || new Date(0, 0, 0, 8, 0, 0),\n    locationStationId: pickupStationList.value[0]?.value,\n  });\n};\nconst addDropOffLocation = () => {\n  props.formData.dropOffLocation.push({\n    runDays: 0,\n    locationTime: props.formData.endTime || new Date(0, 0, 0, 8, 0, 0),\n    locationStationId: dropoffStationList.value[0]?.value,\n  });\n};\nconst getStationList = () => {\n  http.get(\"/operate/busOperateStation/queryList\").then((res) => {\n    const stations = (res.t || []).map((o) => ({\n      value: o.stationId,\n      label: o.stationName,\n      cityId: o.cityId,\n    }));\n    stationList.value = stations;\n    if (!props.formData.pickupLocation.length) {\n      addPickupLocation();\n    }\n    if (!props.formData.dropOffLocation.length) {\n      addDropOffLocation();\n    }\n  });\n};\ngetStationList();\nconst deletePickupLocation = (index) => {\n  props.formData.pickupLocation.splice(index, 1);\n};\nconst deleteDropOffLocation = (index) => {\n  props.formData.dropOffLocation.splice(index, 1);\n};\n\nconst departureCityChange = (val) => {\n  const defaultLocation = pickupStationList.value[0]?.value;\n  props.formData.pickupLocation.forEach((item) => {\n    item.locationStationId = defaultLocation;\n  });\n};\nconst arrivalCityChange = (val) => {\n  const defaultLocation = dropoffStationList.value[0]?.value;\n  props.formData.dropOffLocation.forEach((item) => {\n    item.locationStationId = defaultLocation;\n  });\n};\n\nconst save = () => {\n  ruleForm.value.validate((valid) => {\n    if (valid) {\n      if (props.formData.pricingPlanList.some((o) => !o.schemeId)) {\n        props.formData.isActive = 0;\n      }\n\n      const {\n        buslineId,\n        subBuslineName,\n        startCityId,\n        endCityId,\n        startTime,\n        endTime,\n        runDays,\n        isActive,\n        isSchedule,\n        startScheduleTime,\n        endScheduleTime,\n        isFollow,\n        weekScheduleList,\n        pickupLocation,\n        dropOffLocation,\n        pricingPlanList,\n      } = props.formData;\n\n      const params = {\n        buslineId,\n        subBuslineName,\n        startCityId,\n        endCityId,\n        startTime: dayjs(startTime).format(\"HH:mm\"),\n        endTime: dayjs(endTime).format(\"HH:mm\"),\n        runDays,\n        isActive,\n        isSchedule,\n        startScheduleTime,\n        endScheduleTime,\n        isFollow,\n      };\n      if (!isFollow) {\n        params.weekScheduleList = weekScheduleList;\n      }\n      params.weekScheduleList = weekScheduleList.map((item) => {\n        return {\n          buslineType: 2,\n          isActive: item.isActive,\n          type: item.type,\n        };\n      });\n      params.pricingPlanList = pricingPlanList.map((item) => {\n        return {\n          schemeId: item.schemeId,\n          type: item.type,\n        };\n      });\n      params.locationList = [\n        ...pickupLocation.map((item) => {\n          return {\n            runDays: item.runDays,\n            cityId: startCityId,\n            locationStationId: item.locationStationId,\n            locationTime: dayjs(item.locationTime).format(\"HH:mm\"),\n            type: 1,\n          };\n        }),\n        ...dropOffLocation.map((item) => {\n          return {\n            runDays: item.runDays,\n            cityId: endCityId,\n            locationStationId: item.locationStationId,\n            locationTime: dayjs(item.locationTime).format(\"HH:mm\"),\n            type: 2,\n          };\n        }),\n      ];\n\n      emit(\"save\", params);\n    } else {\n      nextTick(() => {\n        let element = document.querySelector(\".el-form-item__error\");\n        if (element) {\n          let rect = element.getBoundingClientRect();\n          let top = -rect.top - 65;\n          document.querySelector(\"#main\").scrollTo({\n            top,\n            behavior: \"smooth\",\n          });\n        }\n      });\n    }\n  });\n};\nconst cancel = () => {\n  emit(\"cancel\");\n  router.back();\n};\n</script>\n\n<style lang=\"scss\" scoped>\n:deep(.el-form-item) {\n  flex-direction: column;\n  margin-bottom: 10px;\n}\n\n:deep(.el-input) {\n  width: 200px;\n}\n\n.num-select {\n  :deep(.el-input) {\n    width: 80px;\n  }\n}\n\n.el-form-item-center {\n  :deep(.el-form-item__content) {\n    flex-direction: column;\n    align-items: flex-start;\n  }\n}\n\n.el-table {\n  :deep(.el-form-item) {\n    margin-bottom: 0;\n    // margin-top: 22px;\n  }\n\n  :deep(.el-form-item__content) {\n    justify-content: center;\n  }\n}\n\n.ml-30 {\n  margin-left: 30px;\n}\n\n.flex-between {\n  :deep(.el-form-item__label) {\n    justify-content: space-between;\n    padding-right: 0;\n  }\n}\n</style>\n

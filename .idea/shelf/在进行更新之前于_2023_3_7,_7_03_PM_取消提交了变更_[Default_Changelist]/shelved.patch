cPrices.length > 1\"\n                @click=\"deleteDynamicPrice($index)\"\n                >Delete</el-button\n              >\n            </template>\n          </el-table-column>\n        </el-table>\n      </template>\n    </el-form>\n\n    <el-button type=\"primary\" @click=\"save\">Save</el-button>\n    <el-button type=\"primary\" @click=\"cancel\">Cancel</el-button>\n  </div>\n</template>\n\n<script setup>\nimport useCityList from \"@/store/useCityList\";\nimport { ElMessage } from \"element-plus\";\nimport { staticPassengerTypeMap } from \"./staticPassengerType\";\n\nconst props = defineProps({\n  formData: {\n    type: Object,\n    default: () => ({}),\n  },\n});\n\nconst router = useRouter();\nconst cityList = useCityList();\nconst ruleForm = ref(null);\nconst passengerTypeOptions = ref([\n  { label: \"Only Adult\", value: \"1\" },\n  { label: \"Adult,Child\", value: \"12\" },\n  { label: \"Adult,Child,Infant\", value: \"123\" },\n]);\n\nconst staticPricesComputed = computed(() => {\n  const { staticPrices, passengerType, schemeType } = props.formData;\n  if (schemeType !== 1) return [];\n  const passengerTypes = passengerType.split(\"\");\n  return passengerTypes.map((type) => staticPrices[type]);\n});\nconst staticPricesValidator = (staticPrice) => (rule, value, callback) => {\n  if (!staticPrice && parseFloat(staticPrice) !== 0) {\n    callback(new Error(\"Price is required!\"));\n  } else {\n    callback();\n  }\n};\nconst startQuantityValidator = (item, prevItem) => (rule, value, callback) => {\n  if (!item.startQuantity && parseInt(item.startQuantity) !== 0) {\n    const message = \"Start Quantity is required!\";\n    // ElMessage.warning(message);\n    callback(new Error(message));\n  } else if (prevItem && parseInt(item.startQuantity) !== parseInt(prevItem.endQuantity) - 1) {\n    const message = \"Start Quantity must be same as last end quantity minus 1!\";\n    // ElMessage.warning(message);\n    callback(new Error(message));\n  } else {\n    callback();\n  }\n};\nconst endQuantityValidator = (item) => (rule, value, callback) => {\n  if (!item.endQuantity && parseInt(item.endQuantity) !== 0) {\n    const message = \"End Quantity is required!\";\n    // ElMessage.warning(message);\n    callback(new Error(message));\n  } else if (parseInt(item.endQuantity) > parseInt(item.startQuantity)) {\n    const message = \"End Quantity must be less than or equal to start quantity!\";\n    // ElMessage.warning(message);\n    callback(new Error(message));\n  } else {\n    callback();\n  }\n};\nconst dynamicPricesValidator = (item) => (rule, value, callback) => {\n  if (!item.dynamicPrice && parseFloat(item.dynamicPrice) !== 0) {\n    callback(new Error(\"Price is required!\"));\n  } else {\n    callback();\n  }\n};\nconst basicRules = {\n  name: [{ required: true, message: \"Pricing name is required!\", trigger: \"blur\" }],\n  startCityId: [{ required: true, message: \"City is required!\" }],\n  endCityId: [{ required: true, message: \"City is required!\" }],\n};\nconst formRules = computed(() => {\n  const rules = {\n    ...basicRules,\n  };\n\n  staticPricesComputed.value.forEach((item, index) => {\n    rules[`static-price-${index}`] = [\n      {\n        validator: staticPricesValidator(item.staticPrice),\n        trigger: \"blur\",\n      },\n    ];\n  });\n  props.formData.dynamicPrices?.forEach((item, index) => {\n    const prevItem = props.formData.dynamicPrices[index - 1];\n    rules[`start-quantity-${index}`] = [\n      {\n        validator: startQuantityValidator(item, prevItem),\n        trigger: \"blur\",\n      },\n    ];\n    rules[`end-quantity-${index}`] = [\n      {\n        validator: endQuantityValidator(item),\n        trigger: \"blur\",\n      },\n    ];\n    rules[`dynamic-price-${index}`] = [\n      {\n        validator: dynamicPricesValidator(item),\n        trigger: \"blur\",\n      },\n    ];\n  });\n  return rules;\n});\n\nconst addDynamicPrice = () => {\n  // ruleForm.value.validate((valid) => {\n  //   if (valid) {\n  const { dynamicPrices } = props.formData;\n  const { endQuantity } = dynamicPrices[dynamicPrices.length - 1];\n  props.formData.dynamicPrices.push({\n    startQuantity: endQuantity ? parseInt(endQuantity) - 1 : \"\",\n    endQuantity: \"\",\n    price: \"\",\n  });\n  // }\n  // });\n};\n\nconst deleteDynamicPrice = (index) => {\n  props.formData.dynamicPrices.splice(index, 1);\n};\n\nconst emit = defineEmits([\"save\", \"cancel\"]);\nconst save = () => {\n  ruleForm.value.validate((valid) => {\n    if (valid) {\n      const { name, startCityId, endCityId, schemeType, passengerType, dynamicPrices } = props.formData;\n\n      if (schemeType === 2) {\n        const lastItem = dynamicPrices[dynamicPrices.length - 1];\n        if (parseInt(lastItem.endQuantity) !== 0) {\n          const message = \"Last End Quantity must be 0!\";\n          return ElMessage.warning(message);\n        }\n      }\n\n      const params = {\n        name,\n        startCityId,\n        endCityId,\n        schemeType,\n      };\n      if (schemeType === 1) {\n        params.staticPrices = staticPricesComputed.value.map((item) => {\n          return {\n            ...item,\n            // passengerType在前端值为 0 01 012,传给后端为 1 2 3\n            passengerType: passengerType.length,\n          };\n        });\n        params.minPrice = Math.min(...params.staticPrices.map((o) => o.staticPrice));\n        params.roundTripMinPrice = Math.min(...params.staticPrices.map((o) => o.staticRoundtripPrice ?? o.staticPrice));\n      } else if (schemeType === 2) {\n        params.dynamicPrices = dynamicPrices;\n        params.minPrice = Math.min(...dynamicPrices.map((o) => o.dynamicPrice));\n        params.roundTripMinPrice = params.minPrice;\n      }\n\n      emit(\"save\", params);\n    }\n  });\n};\nconst cancel = () => {\n  emit(\"cancel\");\n  router.back();\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.el-table {\n  width: 600px;\n\n  :deep(.el-input) {\n    width: 100px;\n
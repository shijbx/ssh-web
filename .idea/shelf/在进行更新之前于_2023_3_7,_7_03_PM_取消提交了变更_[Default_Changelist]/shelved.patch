detailRef.value.ruleForm.validate((valid) => {\n    if (valid) {\n      const params = { ...formData.value };\n      params.features = params.features.join(\",\");\n      params.startTime = dayjs(params.startTime).format(\"HH:mm\");\n      params.endTime = dayjs(params.endTime).format(\"HH:mm\");\n\n      http.post(\"/operate/busOperateBusline/update\", params).then((res) => {\n        ElMessage.success(\"Updated successfully!\");\n        router.back();\n      });\n    }\n  });\n};\nconst cancel = () => {\n  router.push({ name: \"buslineList\" });\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.title-bar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n</style>\n
